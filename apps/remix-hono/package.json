{
  "author": "",
  "description": "",
  "name": "remix-hono",
  "license": "MIT",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "version": "0.0.0",
  "scripts": {
    "build": "remix vite:build",
    "no-lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "no-dev": "remix vite:dev",
    "no-start": "remix-serve ./build/server/index.js",
    "no-start:docker": "run-s docker:*",
    "no-docker:build": "docker build -t remix-hono-vite .",
    "no-docker:run": "docker run --rm -d -p 8080:8080 --env-file .env remix-hono-vite",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@hono/node-server": "^1.8.2",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "hono": "^4.1.3",
    "isbot": "^5.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remix-hono": "^0.0.16"
  },
  "devDependencies": {
    "@hono/vite-dev-server": "^0.10.0",
    "@remix-run/dev": "^2.8.1",
    "@repo/eslint-config": "*",
    "@repo/typescript-config": "*",
    "@types/react": "^18.2.64",
    "@types/react-dom": "^18.2.21",
    "autoprefixer": "^10.4.18",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.1",
    "esbuild": "^0.20.1",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "~5.3.3",
    "vite": "^5.1.6",
    "vite-tsconfig-paths": "^4.3.1"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
